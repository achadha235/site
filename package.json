  "scripts": {
    "dev": "NODE_ENV=development ts-node -O '{\"module\": \"commonjs\"}' server/index.ts",
    "debug": "NODE_ENV=development node --inspect ./node_modules/.bin/ts-node -O '{\"module\": \"commonjs\"}' server/index.ts ./server/index.ts",
    "build:server": "NODE_ENV=production rm -rf ./dist && tsc --project tsconfig.server.json",
    "build:next": "NODE_ENV=production next build",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "typeorm-migration-generate": "npm run build:server && ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "typeform-migration-run": "npm run build:server && ts-node ./node_modules/.bin/typeorm migration:run",
    "test": "jest --runInBand",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook",
    "clean": "rm -rf data dist build node_modules terraform/production/.terraform",
    "docker:build": "docker build -t webserver ./",
    "minikube:start": "yarn build && minikube start && eval $(minikube docker-env) && yarn docker:build && kubectl config use-context minikube && kubectl apply -f ./k8s/minikube && minikube service webserver-service && minikube dashboard",
    "minikube:stop": "minikube stop && minikube delete",
    "docker:push": "docker tag webserver gcr.io/abhi-250902/webserver && docker push gcr.io/abhi-250902/webserver",
    "db:start": "docker run --publish 54320:5432 --detach --name webserver-db -e POSTGRES_PASSWORD=password -e POSTGRES_USER=root -e POSTGRES_DB=app postgres:11",
    "db:stop": "docker container stop webserver-db && docker container rm webserver-db"
  },
  "devDependencies": {
